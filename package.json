{
  "name": "graphql-tutorial",
  "version": "2.0.0",
  "description": "A project to explore GraphQL written in ES6 with babel and webpack",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf dist dist-prod",
    "watch": "babel src --out-dir dist --source-maps --watch --verbose",
    "serve": "nodemon",
    "build": "webpack --config ./webpack.config.js --mode production",
    "start": "npm run clean && npm ci && npm run build && node dist-prod/bundle.js"
  },
  "keywords": [
    "nodejs",
    "graphql",
    "babel",
    "es6",
    "webpack"
  ],
  "author": "Vincent DAGOURY",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.8",
    "graphql-iso-date": "^3.6.1",
    "lodash": "^4.17.15",
    "loggy": "^1.0.8",
    "mongoose": "^5.7.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.6.0",
    "eslint-loader": "^3.0.2",
    "kill-port": "^1.6.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "prettier-webpack-plugin": "^1.2.0",
    "rimraf": "^3.0.0",
    "source-map": "^0.7.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  }
}
